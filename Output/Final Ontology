ssn:DrinkingWaterDistributionNetwork rdf:type ssn:System.
ssn:Pipe rdf:type ssn:Device.
ssn:Valve rdf:type ssn:Device.
ssn:Pump rdf:type ssn:Device.
ssn:Tank rdf:type ssn:Device.
ssn:Reservoir rdf:type ssn:Device.
ssn:WaterStorage rdf:equivalentClass ssn:Reservoir .
ssn:Channel owl:equivalentClass ssn:Pipe. 
ssn:Faucet owl:equivalentClass ssn:Valve.
ssn:WaterPump owl:equivalentClass ssn:Pump.
ssn:Container owl:equivalentClass ssn:Tank.

# Link properties to classes
ssn:Material rdfs:type :metadataType;

    rdfs:domain ssn:Pipe .
ssn:Diameter rdf:type :metadataValue;
    rdfs:domain ssn:Pipe .
ssn:Length rdf:type :metadataValue;
    rdfs:domain ssn:Pipe .
ssn:RoughnessCoefficient rdf:type :metadataValue;
    rdfs:domain ssn:Pipe .
ssn:Status rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf :metadataType;
    rdfs:domain ssn:Pipe .

ssn:Type rdf:type :metadataType;
    rdfs:domain ssn:Pump,
                ssn:Valve,
                ssn:Reservoir .
ssn:Capacity rdf:type :metadataType;
    rdfs:domain ssn:Pump,
                ssn:Reservoir,
                ssn:Tank .
ssn:PowerConsumption rdf:type :metadataValue;
    rdfs:domain ssn:Pump .
ssn:OperationalStatus rdf:type :metadataType;
    rdfs:domain ssn:Pump,
                ssn:Valve.
ssn:SurfaceArea rdf:type :metadataValue;
    rdfs:domain ssn:Reservoir .
ssn:WaterQuality rdf:type :metadataType;
    rdfs:domain ssn:Reservoir,
                ssn:Tank.
ssn:MinimumWaterLevels rdf:type :metadataValue;
    rdfs:domain ssn:Tank.
ssn:MaximumWaterLevels rdf:type :metadataValue;
    rdfs:domain ssn:Tank.
ssn:SpeedSettings rdf:type :metadataValue;
    rdfs:domain ssn:Pump.
ssn:Settings rdf:type :metadataType;
    rdfs:domain ssn:Valve.
