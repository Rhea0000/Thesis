@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.org/> .

ex:WaterSource rdf:type owl:Class .
ex:PumpStation rdf:type owl:Class .
ex:Reservoir rdf:type owl:Class .
ex:Pipeline rdf:type owl:Class .
ex:Valve rdf:type owl:Class .
ex:Hydrant rdf:type owl:Class .
ex:Meter rdf:type owl:Class .
ex:DistributionNetwork rdf:type owl:Class .

ex:WaterSource rdfs:subClassOf ex:DistributionNetwork .
ex:PumpStation rdfs:subClassOf ex:DistributionNetwork .
ex:Reservoir rdfs:subClassOf ex:DistributionNetwork .
ex:Pipeline rdfs:subClassOf ex:DistributionNetwork .
ex:Valve rdfs:subClassOf ex:DistributionNetwork .
ex:Hydrant rdfs:subClassOf ex:DistributionNetwork .
ex:Meter rdfs:subClassOf ex:DistributionNetwork .